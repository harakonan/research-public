file = {:Users/harakonan/Dropbox/Research/Published/Baba{\_}et{\_}al{\_}2020{\_}VJ{\_}Differential expression of human papillomavirus 16-, 18-, 52-, and 58-derived transcripts in cervical intraepithelial neoplasia.pdf:pdf},
issn = {1743-422X},
journal = {Virology Journal},
