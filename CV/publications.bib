file = {:Users/harakonan/Library/CloudStorage/Dropbox/Research/Published/Kishida{\_}et{\_}al{\_}2022{\_}SR{\_}Impact of lung function impairment after allogeneic hematopoietic stem cell transplantation.pdf:pdf},
